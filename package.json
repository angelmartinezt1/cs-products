{
  "name": "cs-products",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "node --env-file=.env --watch src/index.js",
    "sam": "sam build && sam deploy",
    "build": "echo 'No build step needed'",
    "indexer:run": "node --env-file=.env scripts/run-indexer.js",
    "indexer:full": "node --env-file=.env scripts/run-indexer.js --update-facets --optimize-tables",
    "indexer:quick": "node --env-file=.env scripts/run-indexer.js --start-page 2477 --pages 5398",
    "indexer:stats": "node --env-file=.env scripts/run-indexer.js --stats-only",
    "indexer:dry-run": "node --env-file=.env scripts/run-indexer.js --pages 5 --dry-run",
    "indexer:maintenance": "node --env-file=.env scripts/run-indexer.js --cleanup-old --update-facets --optimize-tables --stats-only",
    "typesense:run": "node --env-file=.env scripts/run-typesense-indexer.js",
    "typesense:full": "node --env-file=.env scripts/run-typesense-indexer.js --recreate-collection",
    "typesense:quick": "node --env-file=.env scripts/run-typesense-indexer.js --pages 10 --start-page 1",
    "typesense:stats": "node --env-file=.env scripts/run-typesense-indexer.js --stats-only",
    "typesense:test": "node --env-file=.env scripts/run-typesense-indexer.js --test-connection",
    "typesense:dry-run": "node --env-file=.env scripts/run-typesense-indexer.js --pages 5 --dry-run",
    "typesense:recreate": "node --env-file=.env scripts/run-typesense-indexer.js --recreate-collection --pages 100",
    "typesense:v2": "node --env-file=.env scripts/typesense-indexer-v2.js",
    "typesense:v2-quick": "node --env-file=.env scripts/typesense-indexer-v2.js --pages 5",
    "typesense:diagnostic": "node --env-file=.env scripts/typesense-diagnostic.js",
    "typesense:existing": "node --env-file=.env scripts/typesense-indexer-existing-schema.js",
    "typesense:existing-quick": "node --env-file=.env scripts/typesense-indexer-existing-schema.js --pages 5",
    "typesense:checkpoint": "node --env-file=.env scripts/typesense-indexer-checkpoint.js",
    "typesense:checkpoint-resume": "node --env-file=.env scripts/typesense-indexer-checkpoint.js --resume",
    "typesense:checkpoint-debug": "node --env-file=.env scripts/typesense-indexer-checkpoint.js --debug-errors --pages 10",
    "typesense:checkpoint-safe": "node --env-file=.env scripts/typesense-indexer-checkpoint.js --stop-on-errors --max-consecutive-errors 3",
    "typesense:detailed-debug": "node --env-file=.env scripts/typesense-indexer-detailed-debug.js",
    "typesense:debug-page": "node --env-file=.env scripts/typesense-indexer-detailed-debug.js --single-page",
    "typesense:fixed": "node --env-file=.env scripts/typesense-indexer-fixed.js",
    "typesense:fixed-resume": "node --env-file=.env scripts/typesense-indexer-fixed.js --resume",
    "typesense:fixed-quick": "node --env-file=.env scripts/typesense-indexer-fixed.js --pages 10"
  },
  "keywords": [],
  "author": "Juan Angel Martínez López",
  "eslintConfig": {
    "extends": "standard"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "mysql2": "^3.14.1",
    "typesense": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^9.30.0",
    "neostandard": "^0.12.1"
  }
}
